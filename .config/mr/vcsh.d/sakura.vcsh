[$HOME/.config/vcsh/repo.d/sakura.git]
checkout = vcsh clone 'git@git.pwnies.dk:IdolfHatler/vcsh-sakura.git' 'sakura' ; \
           vcsh sakura reset --hard origin/master && \
           vcsh sakura checkout -f $HOME
update   = vcsh sakura pull
push     = vcsh sakura push
status   = vcsh sakura status
gc       = vcsh sakura gc

