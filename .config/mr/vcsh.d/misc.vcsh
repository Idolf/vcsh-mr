[$HOME/.config/vcsh/repo.d/misc.git]
checkout = vcsh clone 'git@github.com:Idolf/vcsh-misc.git' 'misc' || \
           (vcsh misc reset --hard origin/master && \
           vcsh misc checkout -f $HOME )
update   = vcsh misc pull
push     = vcsh misc push
status   = vcsh misc status
gc       = vcsh misc gc

