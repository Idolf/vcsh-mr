[$HOME/.config/vcsh/repo.d/gdb.git]
checkout = vcsh clone 'git@git.pwnies.dk:IdolfHatler/vcsh-gdb.git' 'gdb' ; \
           vcsh gdb reset --hard origin/master ; \
           vcsh gdb checkout -f $HOME
update   = vcsh gdb pull
push     = vcsh gdb push
status   = vcsh gdb status
gc       = vcsh gdb gc

