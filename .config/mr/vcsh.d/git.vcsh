[$HOME/.config/vcsh/repo.d/git.git]
checkout = vcsh clone 'git@git.pwnies.dk:Idolf/vcsh-git.git' 'git' || \
           (vcsh git reset --hard origin/master && \
           vcsh git checkout -f $HOME )
update   = vcsh git pull
push     = vcsh git push
status   = vcsh git status
gc       = vcsh git gc

