[$HOME/.config/vcsh/repo.d/bash.git]
checkout = vcsh clone 'git.pwnies.dk:IdolfHatler/vcsh-bash.git' 'bash' && \
           vcsh bash reset --hard origin/master && \
           vcsh bash checkout -f $HOME
update   = vcsh bash pull
push     = vcsh bash push
status   = vcsh bash status
gc       = vcsh bash gc

