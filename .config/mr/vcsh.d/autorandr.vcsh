[$HOME/.config/vcsh/repo.d/autorandr.git]
checkout = vcsh clone 'git.pwnies.dk:IdolfHatler/vcsh-autorandr.git' 'autorandr' && \
           vcsh autorandr reset --hard origin/master && \
           vcsh autorandr checkout -f $HOME
update   = vcsh autorandr pull
push     = vcsh autorandr push
status   = vcsh autorandr status
gc       = vcsh autorandr gc

